{
  "name": "for-await-waterfall",
  "version": "1.0.1",
  "description": "executes async functions synchronously, with for await loop",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --coverage",
    "tdd": "jest --watch",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "clear": "rimraf -rf coverage",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zilberman3k/async-waterfall.git"
  },
  "keywords": [
    "async",
    "waterfall",
    "promises",
    "one after the other",
    "for await waterfall",
    "forEach"
  ],
  "author": "Niv Zilberman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zilberman3k/async-waterfall/issues"
  },
  "homepage": "https://github.com/zilberman3k/async-waterfall#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "eslint": "^6.4.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-node-dev": "^1.0.0-pre.42",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.2"
  },
  "files": [
    "lib/**/*"
  ]
}
